<% if @retorno.first %>
	$('#mensagens').html('');
	$('#mensagens').hide();
	alert('Pesquisa realizada com sucesso! Obrigado pela atenção e volte sempre!');
	<% if current_user.present? %>
		window.location.href = '<%= url_for([:iniciar_pesquisa, :entidade, :auditorias]) %>';
	<% else %>
		window.location.href = '<%= url_for([:root]) %>';
	<% end %>
<% else %>
	alert('<%= @retorno.last %>');
	// $('#mensagens').html('<b><%#= @retorno.last %></b>');
	// $('#mensagens').show();
<% end %>
